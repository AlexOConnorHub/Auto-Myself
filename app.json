{
  "expo": {
    "name": "Auto Myself",
    "slug": "auto-myself",
    "version": "1.0.2",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/transparent-background.png",
      "resizeMode": "contain",
      "backgroundColor": "#A67729"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.automyself.ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transparent-background.png",
        "backgroundColor": "#A67729"
      },
      "package": "com.automyself.android",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "111d2a73-634d-4abf-b2d6-ec1aa4cb5fa3"
      }
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "alex-oconnor"
        }
      ]
    ]
  }
}
